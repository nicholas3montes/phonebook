<table>
    <tr>
        <th>Nome</th>
    </tr>

    <% @contacts.each do |contact| %>
        <tr>
            <td><%=link_to contact.name, contact_path(contact)%></td>
        </tr>
    <% end %>
    
</table>

<div class="button-center">
    <%=button_to "Criar Novo", :new_contact, method: :get%>
</div>


